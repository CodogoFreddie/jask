{
	"name": "todo",
	"private": true,
	"version": "0.0.1",
	"description": "A CLI todo manager",
	"main": "lib/index.js",
	"repository": "git@github.com:CodogoFreddie/todo.git",
	"author": "Freddie Ridell <freddie@codogo.io>",
	"license": "MIT",
	"eslintConfig": {
		"extends": "codogo"
	},
	"babel": {
		"presets": [
			"env"
		],
		"plugins": [
			[
				"transform-object-rest-spread",
				{
					"useBuiltIns": true
				}
			]
		]
	},
	"scripts": {
		"build": "NODE_ENV=production babel src --out-dir lib",
		"format": "react-scripts format",
		"watch": "NODE_ENV=production babel --watch src --out-dir lib"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-node-source-map-support": "^0.0.1",
		"babel-preset-codogo": "^2.0.1",
		"babel-preset-env": "^1.6.1",
		"eslint": "^4.11.0",
		"eslint-config-codogo": "^2.1.0",
		"husky": "^0.14.3",
		"prettier": "^1.8.2"
	},
	"dependencies": {
		"codogo-react-scripts": "^1.0.14-delta.84",
		"ramda": "^0.25.0",
		"redux": "^3.7.2"
	}
}
